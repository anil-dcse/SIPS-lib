    public static void main(String[] args) {
        simulator sim= new simulator();
        int i[]= new int[]{-71,
51,-90,-52,-94,-29,-1,-12,85,-74,-29,
56,-38,-96,-59,-76,-78,-61,71,-46,91,
-40,59,-76,83,56,-82,-24,-94,-52,56,
